<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.tanamgroceryapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".ApplicationInitialize"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TanamGroceryApp"
        tools:ignore="MissingClass">
        <activity
            android:name=".ShippingAddressActivity"
            android:exported="false" />
        <activity
            android:name=".ProductDetailsActivity"
            android:exported="false" />
        <activity
            android:name=".ProductsActivity"
            android:exported="false" />
        <activity android:name=".SearchFilterActivity" />
        <activity android:name=".SearchResultsActivity" />
        <activity android:name=".Home.ProductCategoriesActivity" />
        <activity android:name=".ShoppingCartActivity" />
        <activity android:name=".Home.HomeActivity" />
        <activity android:name=".login.SignUpActivity" />
        <activity android:name=".login.SignInActivity" />
        <activity android:name=".Sreens.SreenActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>